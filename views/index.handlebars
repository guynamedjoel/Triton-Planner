<!doctype html>

<html>
<head>
	<title>TritonPlanner</title>	
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href="/css/bootstrap.min.css" rel="stylesheet">
	<link href="/css/bootstrap-theme.min.css" rel="stylesheet">
	<link href="/css/TritonPlanner.css" rel="stylesheet">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->        
</head>

<body>
	<div class="navbar">
		<ul>
			<li class="left-nav-button"><a href="#"><</a></li>
			<li class="nav-title"><a href="#">Tommy</a></li>
			<li class="right-nav-button"><a href="#">+</a></li>
		</ul>
	</div>

	<div class="container" id="planner">
		{{#each years}}
		<div class="row" id="{{id}}">
			{{#each quarters}}
				<script>yearCSS({{this}});</script>
				<div id="{{id}}"><a href="#">
					<h4>{{quarter}}</h4>
					{{#each classes}}
						<p>{{this}}</p>
					{{/each}}
				</a></div>
			{{/each}}
		</div>
		{{/each}}
	</div>

	<script src="https://code.jquery.com/jquery.js"></script>
	<script src="/js/bootstrap.min.js"></script>
	<script src="/js/TritonPlanner.js"></script>
</body>
</html>
